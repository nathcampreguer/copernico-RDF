<%= form_tag(controller: "search", action: "index") do %>
  <br/>

  <%= label_tag(:search_field, "Search for:") %>
  <%= text_field_tag(:search_field) %>
  <%= submit_tag("Search") %>

  <br/>
  <br/>

  <h5>Metadata results:</h5>

    <% @index.each do |e| %>
        <br/>
      <%= e.join('<br/>').html_safe %>
        <br/>
    <% end %>
<% end %>
